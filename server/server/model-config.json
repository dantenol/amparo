{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongodb",
    "public": false
  },
  "AccessToken": {
    "dataSource": "mongodb",
    "public": false
  },
  "ACL": {
    "dataSource": "mongodb",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongodb",
    "public": true
  },
  "agent": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "login": true,
          "create": true,
          "findById": true,
          "listPsychologists": true,
          "updateAvailability": true,
          "logout": true
        }
      }
    }
  },
  "person": {
    "dataSource": "mongodb",
    "public": false
  },
  "psyRequest": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "form": true,
          "listPatients": true,
          "updateStatusUrgency": true,
          "listLogs": true,
          "addLog": true,
          "newAppointment": true
        }
      }
    }
  },
  "psyDetails": {
    "dataSource": "mongodb",
    "public": false
  },
  "log": {
    "dataSource": "mongodb",
    "public": false
  },
  "psySchedule": {
    "dataSource": "mongodb",
    "public": false
  }
}
